#bash-lib-o
id=("cbash" "2.0" "5507b5af-3439-457d-8123-d3a8bd489529" "https://raw.githubusercontent.com/lnee94/resh/main/l/cbash" )
deps=("insert-package")
defaltrepo="https://raw.githubusercontent.com/lnee94/resh/main/l"
###init-o
eval "$HOOK" ;RESHPACKAGES+=("${id[@]}")
shopt -s expand_aliases
if [ "x$REPO" = "x" ] ;then REPO=$defaltrepo ;fi;depin="resher"
if [ "$(type -t resher)" != "function" ] ;then depinst(){ eval "arg=$2 ; ""$(curl -s $REPO/$1)" ;} ;depin="depinst" ; echo defla;fi
eval "$(echo ${deps[@]} | tr \  \\n | awk "{print \"$depin \"\$1}" )
$(set|grep "^id=");$(set|grep "^arg=") # ;$(set|grep "^deps=" ) "
###init-c
###code-o

case $arg in
    help)
    echo "cbash is a lib for shorenting shell scripts
with a lot and i mean a lot of aliases look at the source code for them
and there is a list of comen link shortenrs 
if you nead something else that is not overly spiftick
send a pull request"
    ;;
    remove)
    unalias r
    unalias m
    unalias c
    unalias W
    unalias v
    unalias M
    unalias X
    unalias l
    unalias S
    unalias w
    unalias F
    unalias A
    unalias C
    unalias CL
    unalias R
    unalias I
    unalias h
    unalias H
    unalias u
    unalias K
    unalias k
    unalias T
    unalias Td
    unalias TD
    unalias IN
    unalias f
    unalias s
    unalias L
    unalias l
    unalias b
    unalias x
    unalias E
    
    unset LS
    unset V
    ;;
    *)
    #the land of aliases
    alias r="rm -r"
    alias m=mkdir
    alias c=cd
    alias W=wget
    alias v=mv
    alias M=chmod
    alias X=chmod\ +x
    alias l=ln
    alias S=ln\ -s
    alias w=while
    alias F=which
    alias A=awk
    alias C=curl\ -s
    alias CL=curl\ -s\ -L
    alias R=arch
    alias I=if
    alias h=do
    alias H=done
    alias u=until
    alias K=then
    alias k=fi
    alias T=tr
    alias Td=tr\ -d
    alias TD=tr\ -d\ \"\\n\"
    alias IN="i=\$((\$i+1))"
    alias f=cat
    alias s=split
    alias L=ls
    alias l=ls\ -1
    alias b=hexdump
    alias x=xxd
    alias E=eval
    #comen vars
    V=( "/dev/null" "/tmp/" )
    #comen link shortenrs
    LS=( "http://tinyurl.com/" "http://bitly.ws/" "http://f.ls/" "http://bit.ly/" "https://youtu.be/" )
    ;;
    esac

###code-c
###exit-o
unset arg
###exit-c
###bash-lib-c  
