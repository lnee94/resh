#bash-lib-o
###makes stuff exacutable


###options-o 
#
#help
#remove
###option-c
case $arg in

  help)
    echo "make execacutable"
    ;;
    
  remove)
    unalias mx
    ;;

  *)
    resher() {
    eval "arg=\$2 ; "'"$(curl -s $REPO/$1 )"'
    }
    ;;
esac


###ed-o
unset arg
###ed-c
###bash-lib-c
