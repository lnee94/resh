
#bash-lib-o
###installs libs



###options-o 
#
#install
#help
#remove
###option-c
case $arg in

  help)
    echo "installs libs"
    ;;
    
  remove)
    unset resher
    ;;
    install)
    resher () {
    eval "arg=$2 ; ""$(curl -s $REPO/$1)"
    } 
    ;;

  *)
    resher () {
    eval "arg=$2 ; ""$(curl -s $REPO/$1)"
    } 
    ;;
esac


###ed-o
unset arg
###ed-c
###bash-lib-c
